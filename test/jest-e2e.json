{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^@auth": "<rootDir>/src/auth/index",
    "^@common/(.*)$": "<rootDir>/src/common/$1",
    "^@journals": "<rootDir>/src/journals/index",
    "^@users": "<rootDir>/src/users/index"
  },
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
